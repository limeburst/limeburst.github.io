---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Jihyeok Seo</title>
  <link href="http://limeburst.net/atom.xml" rel="self"/>
  <link href="http://limeburst.net/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://limeburst.net/</id>
  <author>
    <name>Jihyeok Seo</name>
    <email>me@limeburst.net</email>
  </author>
  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://limeburst.net{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://limeburst.net{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
